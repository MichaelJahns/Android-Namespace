<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flow"
    app:startDestination="@id/pinrose">

    <fragment
        android:id="@+id/pinrose"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.PinroseActivity"
        android:label="fragment_navigation_stab"
        tools:layout="@layout/fragment_pinrose">
        <action
            android:id="@+id/action_navigationStab_to_south"
            app:destination="@id/south"
            app:enterAnim="@anim/from_north"
            app:exitAnim="@anim/to_south"
            app:popEnterAnim="@anim/from_south"
            app:popExitAnim="@anim/to_north" />
        <action
            android:id="@+id/action_navigationStab_to_north"
            app:destination="@id/north"
            app:enterAnim="@anim/from_south"
            app:exitAnim="@anim/to_north"
            app:popEnterAnim="@anim/from_north"
            app:popExitAnim="@anim/to_south" />
        <action
            android:id="@+id/action_pinrose_to_east"
            app:destination="@id/east"
            app:enterAnim="@anim/from_east"
            app:exitAnim="@anim/to_west"
            app:popEnterAnim="@anim/from_west"
            app:popExitAnim="@anim/to_east" />
        <action
            android:id="@+id/action_pinrose_to_west"
            app:destination="@id/west"
            app:enterAnim="@anim/from_west"
            app:exitAnim="@anim/to_east"
            app:popEnterAnim="@anim/from_east"
            app:popExitAnim="@anim/to_west" />
        <action
            android:id="@+id/action_pinrose_to_northWest"
            app:destination="@id/northWest"
            app:enterAnim="@anim/from_southwest"
            app:exitAnim="@anim/to_northeast"
            app:popEnterAnim="@anim/from_northeast"
            app:popExitAnim="@anim/from_southwest" />
        <action
            android:id="@+id/action_pinrose_to_northEast"
            app:destination="@id/northEast"
            app:enterAnim="@anim/from_southeast"
            app:exitAnim="@anim/to_northwest"
            app:popEnterAnim="@anim/from_northwest"
            app:popExitAnim="@anim/to_southeast"  />
        <action
            android:id="@+id/action_pinrose_to_southEast"
            app:destination="@id/southEast"
            app:enterAnim="@anim/from_northeast"
            app:exitAnim="@anim/to_southwest"
            app:popEnterAnim="@anim/from_southwest"
            app:popExitAnim="@anim/to_northeast" />
        <action
            android:id="@+id/action_pinrose_to_southWest"
            app:destination="@id/southWest"
            app:enterAnim="@anim/from_northwest"
            app:exitAnim="@anim/to_southeast"
            app:popEnterAnim="@anim/from_southeast"
            app:popExitAnim="@anim/to_northwest" />
    </fragment>
    <fragment
        android:id="@+id/north"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.north"
        android:label="fragment_north"
        tools:layout="@layout/fragment_north">
        <action
            android:id="@+id/action_north_to_navigationStab"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_north"
            app:exitAnim="@anim/to_south"
            app:popEnterAnim="@anim/from_south"
            app:popExitAnim="@anim/to_north" />
    </fragment>
    <fragment
        android:id="@+id/south"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.south"
        android:label="fragment_south"
        tools:layout="@layout/fragment_south">
        <action
            android:id="@+id/action_south_to_navigationStab"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_south"
            app:exitAnim="@anim/to_north"
            app:popEnterAnim="@anim/from_north"
            app:popExitAnim="@anim/to_south" />
        <action
            android:id="@+id/action_south_to_bounce2"
            app:destination="@id/bounce"
            app:enterAnim="@anim/bounce"
            app:exitAnim="@anim/still"
            app:popEnterAnim="@anim/from_north"
            app:popExitAnim="@anim/to_south" />
    </fragment>
    <fragment
        android:id="@+id/east"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.east"
        android:label="fragment_east"
        tools:layout="@layout/fragment_east">
        <action
            android:id="@+id/action_east_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_west"
            app:exitAnim="@anim/to_east"
            app:popEnterAnim="@anim/from_east"
            app:popExitAnim="@anim/from_west" />
    </fragment>
    <fragment
        android:id="@+id/west"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.west"
        android:label="fragment_west"
        tools:layout="@layout/fragment_west">
        <action
            android:id="@+id/action_west_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_east"
            app:exitAnim="@anim/to_west"
            app:popEnterAnim="@anim/from_west"
            app:popExitAnim="@anim/to_east" />
    </fragment>
    <fragment
        android:id="@+id/northWest"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.NorthWest"
        android:label="fragment_north_west"
        tools:layout="@layout/fragment_north_west">
        <action
            android:id="@+id/action_northWest_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_northeast"
            app:exitAnim="@anim/to_southwest"
            app:popEnterAnim="@anim/from_southwest"
            app:popExitAnim="@anim/to_northeast" />
    </fragment>
    <fragment
        android:id="@+id/northEast"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.NorthEast"
        android:label="fragment_north_east"
        tools:layout="@layout/fragment_north_east">
        <action
            android:id="@+id/action_northEast_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_northwest"
            app:exitAnim="@anim/to_southeast"
            app:popEnterAnim="@anim/from_southeast"
            app:popExitAnim="@anim/to_northwest"
  />
    </fragment>
    <fragment
        android:id="@+id/southEast"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.SouthEast"
        android:label="fragment_south_east"
        tools:layout="@layout/fragment_south_east">
        <action
            android:id="@+id/action_southEast_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_southwest"
            app:exitAnim="@anim/to_northeast"
            app:popEnterAnim="@anim/from_northeast"
            app:popExitAnim="@anim/to_southwest"/>
    </fragment>
    <fragment
        android:id="@+id/southWest"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.SouthWest"
        android:label="fragment_south_west"
        tools:layout="@layout/fragment_south_west">
        <action
            android:id="@+id/action_southWest_to_pinrose"
            app:destination="@id/pinrose"
            app:enterAnim="@anim/from_southeast"
            app:exitAnim="@anim/to_northwest"
            app:popEnterAnim="@anim/from_northwest"
            app:popExitAnim="@anim/to_southeast"
             />
    </fragment>
    <fragment
        android:id="@+id/bounce"
        android:name="com.michaeljahns.namespace.Pinrose.Directions.bounce"
        android:label="fragment_bounce"
        tools:layout="@layout/fragment_bounce" >
        <action
            android:id="@+id/action_bounce2_to_south"
            app:destination="@id/south"
            app:enterAnim="@anim/from_north"
            app:exitAnim="@anim/to_south"
            app:popEnterAnim="@anim/from_south"
            app:popExitAnim="@anim/to_north" />
    </fragment>
</navigation>